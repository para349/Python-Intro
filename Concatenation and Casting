#Concatenation and Casting
first_name = "Alex"
last_name = "Mcleod-Stene"
birth_year = 2006
current_year = 2023

#print("Hello, my name is", first_name, last_name + ",", "I am", current_year - birth_year, "years old")
print("Hello, my name is " + first_name + " " + last_name + ", I am " + str(current_year - birth_year) + " years old.")

