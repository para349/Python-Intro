a = 10
b = 40
#Add two numbers
print(a+b)
#Subtract two numbers
print(a-b)
#Multiply two numbers
print(a*b)
#Divide two numbers and return as a float (decimal)
print(a/b)
#Divide two numbers and return the answer as INT
print(a//b)
#Divide two numbers and return the rema
print(a%b)

also
a = 10
b = 40
b = 10
print(a+b)
